// 4. Напишите программу, которая запрашивает у пользователя число 
// и выводит на экран таблицу умножения для этого числа от 1 до 10.
#include <iostream>
using namespace std;


int main()
{
	setlocale(LC_ALL, "Russian");
	int a;
	cout << "Введите любое число\n";
	cin >> a;

	for (int i = 1; i < 11; i++) {
		cout << a << " * " << i << " = " << a * i << endl;
	}
}
