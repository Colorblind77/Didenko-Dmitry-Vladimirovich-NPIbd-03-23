#include <iostream>
using namespace std;

void Power1234(float a, float &b,float &c, float &d) {
	b = a * a;
	c = a * a * a;
	d = a * a * a * a;
}

int main()
{
	setlocale(LC_ALL, "Russian");
	float a,b,c,d;
	for (int i = 0; i < 5; i++){

		cin >> a;
		Power1234(a, b, c, d);
		cout << b<<' '<<c<<' '<<d<<endl;
	}

}
